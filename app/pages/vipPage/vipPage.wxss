}

/** vip活动 **/
.vip-activity {
    margin: 82rpx auto 38rpx;
    width: 690rpx;
    height: 220rpx;
    background-color: #F9E7D4;
    background: url(https://image.tairanmall.com/f310ed3f9d31745d77f11e25b96414da5d64eec22517b) no-repeat center center;
    background-size: 690rpx 220rpx;
}

/**  推荐商品  **/
.vip-goods {
    width: 100%;
    background-color: rgba(250,227,205, 0.3);
    padding-top: 40rpx;
    padding-bottom: 28rpx;
}
.vip-goods .vip-goods-title {
    display: flex;
    width: 100%;
    justify-content: center;
    align-items: center;
    color: #9A6B4B;
    margin-bottom: 32rpx;
}
.title-line {
    width: 160rpx;
    height: 4rpx;
}
.vip-goods .vip-goods-title text{
    display: inline-block;
    color: #9A6B4B;
    margin: 0 18rpx;
}
.vip-goods .goods-swipe-wrapper{

}
.vip-goods .goods-swipe-wrapper{
    overflow: hidden;
}
.vip-goods .goods-swipe-wrapper .swipe-box {
    width:100%;
}
.vip-goods .goods-swipe-wrapper .swipe-content{
    display:-webkit-box;
    display:-ms-flexbox;
	display:flex;
    padding: 0 6rpx;

}
.vip-goods .goods-swipe-wrapper .item{
    position: relative;
    display: inline-block;
    text-align: center;
	width: 200rpx;
	margin: 0 20rpx;
}

.vip-goods .goods-swipe-wrapper .item .more-box{
    padding: 32rpx 16rpx 0 20rpx;
}

.vip-goods .goods-swipe-wrapper .item .more-box .more-img {
    width: 164rpx;
    height: 160rpx;
    background-color: #FCF3E3;

}

.vip-goods .goods-swipe-wrapper .item .more-box .more-text {
    margin: 14rpx auto 0;
    height: 42rpx;
    line-height: 42rpx;
    width: 120rpx;
    text-align: center;
    color: #999999;
    font-size: 22rpx;
    border-radius: 22rpx;
    border: 1px solid rgba(153,153,153,0.5);
}


.vip-goods .goods-swipe-wrapper .item .img{
	width: 200rpx;
	height: 200rpx;
}
.vip-goods .goods-swipe-wrapper .item .title{
    height: 32rpx;
	line-height: 32rpx;
	font-size: 24rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.vip-goods .goods-swipe-wrapper .item .price{
	text-align:center;
	color: #D81317;
	height: 30rpx;
	line-height: 30rpx;
	font-size: 26rpx;
}


.vip-goods .goods-swipe-wrapper .item .discount{
    display: inline-block;
    height: 32rpx;
    padding: 0 10rpx;
    line-height: 32rpx;
    text-align: center;
    background-color: #353535;
    color: #F9F2EB;
    font-size: 20rpx;
    border-radius: 16rpx;
}

/** 协议 **/
.agreement-box {
    display: flex;
    height: 80rpx;
    width: 100%;
    flex-direction: row;
    justify-content: center;
    align-items: center;

}
.agreement-icon-tap {
    display: flex;
    justify-content: start;
    align-items: center;
}
.agreement-box .agreement-icon {
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
}
.agreement-box .not-agreement-icon {
    border: 1px solid #353535;

}
.agreement-box .agreement-text1 {
    padding-left: 16rpx;
    color: #333333;
    font-size: 24rpx;
}

.agreement-box .agreement-text2 {
    margin-left: 4rpx;
    color: #2876E1;
    font-size: 24rpx;
}
.open-btn {
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
    background-color: #F7DBBD;
    color: #9A6B4B;
    font-size: 36rpx;
    font-weight: bold;
}
.open-disable{
  background-color: #c9c9c9;
  color: #fff
}

/** 邀请人信息 **/
.inviter-info-popup {
    position: absolute;
    z-index: 12;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 560rpx;
    height: 460rpx;
    border-radius: 18rpx;
    background-color: #ffffff;
}
.inviter-info-popup .inviter-popup-box {
    position: relative;
}
.inviter-info-popup .inviter-popup-title {
    padding-top: 50rpx;
    text-align: center;
    font-size: 34rpx;
    color: #473728;
}
.inviter-info-popup .inviter-popup-code {
    margin-top: 60rpx;
    padding: 26rpx 94rpx;
}

.inviter-info-popup .inviter-popup-code input {
    font-size: 26rpx;
    color: #473728;
}
.inviter-info-popup .inviter-popup-btn{
    margin-top: 50rpx;
    margin-left: 60rpx;
    width: 440rpx;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    border-radius: 44rpx;
    background: linear-gradient(to right, #D09B5A, #EECFB1);
    color: #473728;
    font-size: 36rpx;
}
.inviter-info-popup .inviter-popup-close{
    position: absolute;
    right: 0;
    top: -102rpx;
    width: 62rpx;
    height: 62rpx;
}