/* banner */

.banner-wraper {
  width: 100%;
  position: relative;
}

.banner-swiper {
  height:335rpx;
}

.banner-img {
  width: 100%;
  height: 100%;
}

.banner-dots {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 56rpx;
  height: 32rpx;
  border-radius: 32rpx;
  background: rgba(0,0,0,0.5);
  color: #ffffff;
  text-align: center;
  font-size: 24rpx;
  line-height: 31rpx;
}