.share-earn {
    position: fixed;
    right: 20rpx;
    bottom: 300rpx;
    padding: 10rpx 12rpx;
    display: flex;
    align-items: center;
    background: -webkit-linear-gradient(#ffe9b7, #f6d281);
    border-radius: 40rpx;
    z-index: 1000;
}
.share-earn>image{
    width:10rpx;
    height:18rpx;
    margin-left:10rpx
}
.share-earn>view:nth-of-type(1){
    font-size:26rpx;color:#643C14
}
.share-earn>view:nth-of-type(2){
    display:flex;flex-direction: column;font-size:20rpx;color:#643C14
}
.share-earn>text{
    color:#c9c9c9;
    font-size:14rpx;
    margin:10rpx
}
