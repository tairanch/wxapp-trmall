/* common */
@keyframes turnRound{
	0%{
		transform: rotate(0);
	}
	100%{
		transform: rotate(360deg);
	}
}

.c-vh{visibility:hidden;}
.c-fs10{  font-size:10px; }
.c-fs12{  font-size:12px; }
.c-fs15{  font-size:15px; }
.c-c999{ color:#999; }
.c-bgc9{background:#c9c9c9;}
.c-bgf44{background:#ff4444;}
.c-cf00{ color:#ff0000; }
.c-bgdred{background:#e60a30;}
.c-tc{  text-align:center;  }

/* mask */
.mask{
	width: 100%;
	height: 100%;
	position: fixed;
	left:0;
	top:0;
	z-index:120;
}
.mask .mask-load{
	width:48rpx;
	height:48rpx;
	position: absolute;
	top:50%;
	left: 50%;
	transform: translate3d(-50%, -50%, 0);
}

